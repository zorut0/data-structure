#include<stdio.h>
int main()
{
  int a1[3][3],a2[3][3],a3[3][3];
  int i,j,k,sum=0;
  printf("Enter elements in matrix1 : \n");
  for(i=0;i<3;i++)
  {
    for(j=0;j<3;j++)
    {
      scanf("%d",&a1[i][j]);
    }
  }
  printf("Enter elements in matrix2 : \n");
  for(i=0;i<3;i++)
  {
    for(j=0;j<3;j++)
    {
      scanf("%d",&a2[i][j]);
    }
  }
  for(i=0;i<3;i++) //mutliplication starts
  {
    for(j=0;j<3;j++)
    {
        sum=0;
      for(k=0;k<3;k++)
      {
        sum=sum+a1[i][k]*a2[k][j];
      }
      a3[i][j]=sum;
    }
  }
  for(i=0;i<3;i++)
  {
    for(j=0;j<3;j++)
    {
      printf("%d ",a3[i][j]);
    }
    printf("\n");
  }
  return 0;
}
