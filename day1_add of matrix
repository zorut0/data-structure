#include<stdio.h>
int main()
{
  int a1[3][3],a2[3][3],a3[3][3];
  int i,j;
  printf("Enter elements in matrix1 : ");
  for(i=0;i<3;i++)
  {
    for(j=0;j<3;j++)
    {
      scanf("%d",&a1[i][j]);
    }
  }
  printf("Enter elements in matrix2 : ");
  for(i=0;i<3;i++)
  {
    for(j=0;j<3;j++)
    {
      scanf("%d",&a2[i][j]);
    }
  }
  for(i=0;i<3;i++)
  {
    for(j=0;j<3;j++)
    {
      a3[i][j]=a1[1][j]+a2[i][j];
    }
  }
  for(i=0;i<3;i++)
  {
    for(j=0;j<3;j++)
    {
      printf("%d ",a3[i][j]);
    }
    printf("\n");
  }
  return 0;
}
