#include<stdio.h>
int main()
{
  int arr[3][3];
  int i,j;
  printf("\nEnter elements of array: \n");
  for(i=0;i<3;i++)
  {
    for(j=0;j<3;j++)
    {
      scanf("%d",&arr[i][j]);
    }
  }
  printf("\nOriginal matrix:\n");
  for(i=0;i<3;i++)
  {
    for(j=0;j<3;j++)
    {
      printf("%d ",arr[i][j]);
    }
    printf("\n");
  }
  printf("\nTranspose matrix:\n");
  for(i=0;i<3;i++)
  {
    for(j=0;j<3;j++)
    {
      printf("%d ",arr[j][i]);
    }
    printf("\n");
  }
  printf("RgNo: 192325031");
  return 0;
}

