#include<stdio.h>
#define MAX_SIZE 100
int main()
{
  int arr[MAX_SIZE];
  int n,i,j,max;
  printf("Enter size of the array : ");
  scanf("%d",&n);
  printf("Enter elements in array : ");
  for(i=0;i<n;i++)
  {
    scanf("%d",&arr[i]);
  }
  max=arr[0];
  for(i=0;i<n;i++)
  {
    if(max<arr[i])
    {
      max=arr[i];
    }
  }
  printf("\nLargest Element in the Array : %d",max);
  printf("RgNo: 192325031");
  return 0;
}
